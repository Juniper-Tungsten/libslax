version 1.1;

match configuration {
    var $name-servers = name-servers/name;
    
    call temp:ting();
    call temp:ting($name-servers, $size = count($name-servers));
    call temp:ting($name-servers, $size = count($name-servers));
}

template temp:ting ($name-servers, $size = 0) {
    <output> "template called with size " _ $size;
}
