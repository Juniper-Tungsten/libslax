version 1.0;

match / {
    var $inventory = inventory;
    
    for-each ($inventory/chassis/chassis-module/chassis-sub-module[part-number == "750-000610"]) {
        <message> "Down rev PIC in " _ ../name _ ", " _ name _ ": " _ description;
    }
}
