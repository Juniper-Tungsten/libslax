/* Machine Crafted with Care (tm) by slaxWriter */
version 1.1;

param $full;

match / {
    <top> {
        if ($full) {
            var $host = slax:get-input("Please enter hostname: ");
            var $pass = slax:get-secret("Please enter password: ");
            
            <host> $host;
            <pass> $pass;
            var $list = {
                for-each (1 ... 10) {
                    <cmd> slax:get-command("$ ");
                }
            }
            <list> {
                copy-of $list;
            }
        }
    }
}
