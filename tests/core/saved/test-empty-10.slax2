/* Machine Crafted with Care (tm) by slaxWriter */
version 1.1;

output-method xml {
    indent "yes";
}

match / {
    <op-script-results> {
        <output> format-number(1.15, "####.#");
        <output> format-number(10.15, "####.#");
        <output> format-number(100.15, "####.#");
        <output> format-number("100.15", "####.#");
        <output> format-number(1000.15, "####.#");
        <output> format-number("100.15", "####.################");
        <output> format-number(100.15, "####.################");
        <output> format-number(number("100.15"), "####.################");
    }
}
