version 1.1;

match / {
    <top> {
        var $a = {
            call test($a = 1);
        }
        var $b := {
            call test($a = 2);
        }
        var $c = call/call/call;
        var $d = {
            call test($a = 3);
        }
        var $e = call _ call _ call;
        var $f = {
            call _();
        }
        
        <a> $a;
        <b> $b;
        <c> $c;
        <d> $d;
        <e> $e;
        <f> $f;
    }
}

template test ($a = "A") {
    <output> $a;
}

template _ ($a = "A") {
    <output> $a;
}
