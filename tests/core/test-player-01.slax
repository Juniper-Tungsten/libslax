version 1.0;

output-method {
    indent "yes";
}

match /roster {
    <out> {
	var $x = {
	    apply-templates;
	}
        <x> { copy-of $x; }
    }
}

match player {
    if (salary) {
	<player last=last first=first> {
	    <salary> salary;
	    var $salary = salary;
	    <count> count(../player[salary > $salary]);
	}
    }
}

match text() { }
