version 1.0;

match doc {
    var $node = .;
    
    <xsl:element name="Document" namespace=namespace-uri(.)> {
        for-each (namespace::*) {
            if (string(.) != namespace-uri($node)) {
                <xsl:copy>;
            }
        }
    }
}
