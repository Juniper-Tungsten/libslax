/* Machine Crafted with Care (tm) by slaxWriter */
version 1.0;

match doc {
    var $node = .;
    
    element "Document" {
        ns-template namespace-uri(.);
        
        for-each (namespace::*) {
            if (string(.) != namespace-uri($node)) {
                copy-node;
            }
        }
    }
}
