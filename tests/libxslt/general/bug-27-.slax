version 1.0;

attribute-set my-attr-set {
    attribute "attr" {
        expr "from-attr-set";
    }
}

match / {
    <p xsl:use-attribute-sets="my-attr-set" attr="from-p-tag"> {
        attribute "attr" {
            expr "from-xsl-attr";
        }
    }
}
