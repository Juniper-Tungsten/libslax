version 1.1;

output-method xml {
    indent "yes";
}

match / {
    <result> {
        apply-templates;
    }
}

key k1 {
    match entry;
    value @cat1;
}

key k2 {
    match entry;
    value @cat2;
}

match list {
    var $k = {
        if (@type == "one") {
            expr "k1";
        
        } else {
            expr "k2";
        }
    }
    
    <total> {
        <type> @type;
        <a> sum(key($k, "a")/@qty);
        <b> sum(key($k, "b")/@qty);
    }
}

match entries { }
