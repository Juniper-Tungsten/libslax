version 1.0;

ns test = "http://www.example.org/test";

output-method xml {
    encoding "UTF-8";
    indent "yes";
}

key k {
    match test:a;
    value @a;
}

key k {
    match test:b;
    value @b;
}

key k {
    match test.a;
    value @a;
}

match / {
    <test:data> {
        for-each (key("k", "1")) {
            copy-of .;
        }
    }
}

match text() |@* { }
<test:data> {
    <test:a a="1">;
    <test:b b="1">;
}
