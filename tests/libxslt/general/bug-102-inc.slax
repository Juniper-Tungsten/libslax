version 1.0;

output-method html;

attribute-set foo-attribs {
    attribute "size" {
        expr "+1";
    }
}

attribute-set foo-dep-attribs {
    use-attribute-sets foo-attribs;
    attribute "text-align" {
        expr "start";
    }
}

match / {
    <font xsl:use-attribute-sets="foo-dep-attribs"> "\n    foo\n    ";
}
