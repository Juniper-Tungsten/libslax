version 1.0;

ns xs = "http://www.w3.org/2001/XMLSchema";

output-method xml {
    indent "yes";
    version "1.0";
}

match node() {
    element "toto" {
        attribute "n" {
            expr name();
        }
        apply-templates;
    }
}

match text() { }

match comment() { }

match processing-instruction() { }
