<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <!-- Machine Crafted with Care (tm) by slaxWriter -->
  <xsl:strip-space elements="*"/>
  <xsl:output method="html" indent="no"/>
  <xsl:template match="/">
    <div class="errataset">
      <xsl:text>
</xsl:text>
      <xsl:for-each select="errataset/errata">
        <xsl:sort select="@page" data-type="number"/>
        <xsl:sort select="@paragraph" data-type="number"/>
        <xsl:sort select="@line" data-type="number"/>
        <xsl:apply-templates select="."/>
      </xsl:for-each>
    </div>
  </xsl:template>
  <xsl:template name="errata" match="errata">
    <div class="errata">
      <xsl:text>
</xsl:text>
      <xsl:call-template name="show-attributes"/>
      <xsl:for-each select="* | text()">
        <xsl:apply-templates select="."/>
      </xsl:for-each>
    </div>
    <xsl:text>
</xsl:text>
  </xsl:template>
  <xsl:template match="text()">
    <xsl:if test="string-length(normalize-space(.))">
      <p style="clear: both; padding: 3px;">
        <xsl:value-of select="normalize-space(.)"/>
      </p>
      <xsl:text>
</xsl:text>
    </xsl:if>
  </xsl:template>
  <xsl:template name="explanation" match="explanation">
    <blockquote class="explanation">
      <xsl:text>
</xsl:text>
      <p>
        <xsl:text>
</xsl:text>
        <xsl:value-of select="normalize-space(text())"/>
        <xsl:text>
</xsl:text>
      </p>
      <xsl:text>
</xsl:text>
    </blockquote>
    <xsl:text>
</xsl:text>
  </xsl:template>
  <xsl:template name="show-attributes">
    <table class="location-type" cellspacing="0" cellpadding="2">
      <xsl:text>
</xsl:text>
      <xsl:choose>
        <xsl:when test="@paragraph">
          <xsl:choose>
            <xsl:when test="@line">
              <tr>
                <td style="width: 15%;" class="page">
                  <xsl:text>Page: </xsl:text>
                  <xsl:value-of select="@page"/>
                </td>
                <xsl:text>
</xsl:text>
                <td style="width: 15%;" class="paragraph">
                  <xsl:text>Paragraph: </xsl:text>
                  <xsl:value-of select="@paragraph"/>
                </td>
                <xsl:text>
</xsl:text>
                <td style="width: 15%;" class="line">
                  <xsl:text>Line: </xsl:text>
                  <xsl:value-of select="@line"/>
                </td>
                <xsl:text>
</xsl:text>
                <td style="width: 55%;" class="type">
                  <xsl:value-of select="@type"/>
                </td>
                <xsl:text>
</xsl:text>
              </tr>
              <xsl:text>
</xsl:text>
            </xsl:when>
            <xsl:otherwise>
              <tr>
                <td style="width: 15%;" class="page">
                  <xsl:text>Page: </xsl:text>
                  <xsl:value-of select="@page"/>
                </td>
                <xsl:text>
</xsl:text>
                <td style="width: 15%;" class="paragraph">
                  <xsl:text>Paragraph: </xsl:text>
                  <xsl:value-of select="@paragraph"/>
                </td>
                <xsl:text>
</xsl:text>
                <td style="width: 70%;" class="type">
                  <xsl:value-of select="@type"/>
                </td>
                <xsl:text>
</xsl:text>
              </tr>
              <xsl:text>
</xsl:text>
            </xsl:otherwise>
          </xsl:choose>
        </xsl:when>
        <xsl:when test="@line">
          <tr>
            <xsl:text>
</xsl:text>
            <td style="width: 15%;" class="page">
              <xsl:text>Page: </xsl:text>
              <xsl:value-of select="@page"/>
            </td>
            <xsl:text>
</xsl:text>
            <td style="width: 15%;" class="line">
              <xsl:text>Line: </xsl:text>
              <xsl:value-of select="@line"/>
            </td>
            <xsl:text>
</xsl:text>
            <td style="width: 70%;" class="type">
              <xsl:value-of select="@type"/>
            </td>
            <xsl:text>
</xsl:text>
          </tr>
          <xsl:text>
</xsl:text>
        </xsl:when>
        <xsl:otherwise>
          <tr>
            <td style="width: 15%;" class="page">
              <xsl:text>Page: </xsl:text>
              <xsl:value-of select="@page"/>
            </td>
            <xsl:text>
</xsl:text>
            <td style="width: 85%;" class="type">
              <xsl:value-of select="@type"/>
            </td>
            <xsl:text>
</xsl:text>
          </tr>
          <xsl:text>
</xsl:text>
        </xsl:otherwise>
      </xsl:choose>
    </table>
    <xsl:text>
</xsl:text>
  </xsl:template>
</xsl:stylesheet>
