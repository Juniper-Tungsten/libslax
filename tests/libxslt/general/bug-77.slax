version 1.0;


/* I have tried this with   xmlns:xml="http://www.w3.org/XML/1998/namespace"
defined above, too
 */
match / {
    apply-templates {
        mode "copy";
    }
}

match @* | node() {
    mode "copy";
    
    copy-node {
        apply-templates node() |@* {
            mode "copy";
        }
    }
}
