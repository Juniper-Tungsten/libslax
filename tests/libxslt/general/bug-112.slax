version 1.0;

ns exsl exclude extension = "http://exslt.org/common";

match / {
    var $tds = <row> {
        <cell> "a";
        <cell> "b";
        <cell> "c";
        <cell> "d";
    }
    
    for-each (exsl:node-set($tds)/row/cell) {
        expr .;
    }
}
