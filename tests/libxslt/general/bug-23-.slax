version 1.0;

output-method xml {
    indent "yes";
}

match / {
    <indexdoc> {
        apply-templates //indexterm {
            sort primary {
                data-type "text";
            }
            sort secondary {
                data-type "text";
            }
        }
    }
}

match indexterm {
    <term> {
        expr primary;
        expr ":";
        expr secondary;
    }
}
