version 1.1;

output-method {
    encoding "utf-8";
}

match table {
    <table> {
        apply-templates;
    }
}

match row {
    <row> {
        apply-templates;
    }
}

match * {
    element "column" {
        attribute "name" {
            expr name(.);
        }
        attribute "value" {
            expr .;
        }
    }
}
