version 1.0;

<xsl:output encoding="utf-8">;

match table {
    <table> {
        apply-templates;
    }
}

match row {
    <row> {
        apply-templates;
    }
}

match * {
    <xsl:element name="column"> {
        <xsl:attribute name="name"> name(.);
        <xsl:attribute name="value"> .;
    }
}
