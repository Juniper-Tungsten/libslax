/* Machine Crafted with Care (tm) by slaxWriter */
version 1.0;

output-method {
    encoding "utf-8";
}

attribute-set attset1 {
    attribute "att1" {
        expr "foo";
    }
    attribute "att2" {
        expr "bar";
    }
}

attribute-set attset2 {
    attribute "att3" {
        expr "baz";
    }
    attribute "att4" {
        expr "quux";
    }
}

match salesdata | year {
    copy-node {
        use-attribute-sets attset1;
        apply-templates;
    }
}

match region {
    copy-node {
        use-attribute-sets attset2;
        apply-templates;
    }
}

match name | sales {
    copy-node {
        use-attribute-sets attset1 attset2;
        apply-templates;
    }
}
