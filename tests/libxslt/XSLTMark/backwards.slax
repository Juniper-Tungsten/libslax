version 1.0;

/* reassembles an xml tree in reverse order */
<xsl:output encoding="utf-8">;

match * {
    <xsl:copy> {
        apply-templates node() {
            <xsl:sort select="position()" data-type="number" order="descending">;
        }
    }
}
