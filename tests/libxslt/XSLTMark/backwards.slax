/* Machine Crafted with Care (tm) by slaxWriter */
version 1.1;

/* reassembles an xml tree in reverse order */
output-method {
    encoding "utf-8";
}

match * {
    copy-node {
        apply-templates node() {
            sort position() {
                data-type "number";
                order "descending";
            }
        }
    }
}
