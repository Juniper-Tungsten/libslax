version 1.1;

ns fo = "http://www.w3.org/1999/XSL/Format";

match H4 {
    <fo:block> {
        number {
            level "any";
            from H1;
            count H2;
        }
        expr ".";
        number {
            level "any";
            from H2;
            count H3;
        }
        expr ".";
        number {
            level "any";
            from H3;
            count H4;
        }
        expr " ";
        apply-templates;
    }
}
