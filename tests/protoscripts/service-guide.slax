version 1.1;

ns axsl = "http://www.w3.org/1999/XSL/TransformAlias";
ns-alias axsl xsl;

match / {
    var $top = /xsl:stylesheet/service-guide;
    var $gen = /xsl:stylesheet/generate;
    <axsl:stylesheet version="1.0"> {
        ns slax-ext = "http://xmlsoft.org/XSLT/namespace";
	<axsl:import href="../import/junos.xsl">;
	<axsl:import href="../protoscripts/service-guide-lib.slax">;

        <axsl:variable name="guide-temp-1"> {
            copy-of $top/guide;
        }            
        <axsl:variable name="guide" select="slax-ext:node-set($guide-temp-1)">;
        <axsl:variable name="title"> {
            attribute "select" {
                expr "'" _ $top/title _ "'";
            }
        }
        <axsl:variable name="group"> {
            attribute "select" {
                expr "'" _ $gen/@group _ "'";
            }
        }
    }
}
